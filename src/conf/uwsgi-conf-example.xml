<uwsgi>
<env>DJANGO_SETTINGS_MODULE=settings</env>
<wsgi-file>wsgi.py</wsgi-file>
<socket>/var/run/web/bonuscard.sock</socket>
<pythonpath>/path/src</pythonpath>
<procname-master>bonuscard-uwsgi</procname-master>
<procname>bonuscard-uwsgi-worker</procname>
<listen>32</listen>
<max-requests>100</max-requests>
<optimize>2</optimize>
<workers>2</workers>
<idle>30</idle>
<prio>0</prio>
<uid>www-data</uid>
<gid>www-data</gid>
<vacuum/>
<autoload/>
<single-interpreter/>
<master/>
<socket-timeout>60</socket-timeout>
<harakiri>30</harakiri>
<buffer-size>4096</buffer-size>
</uwsgi>
